    "# Dictionary with years as keys and playlist IDs as values\n",
    "playlist_ids = {\n",
    "    2013: '37i9dQZF1DX3Sp0P28SIer',\n",
    "    2014: '37i9dQZF1DX0yEZaMOXna3',\n",
    "    2015: '37i9dQZF1DX9ukdrXQLJGZ',\n",
