    "    2022: '56r5qRUv3jSxADdmBkhcz7'\n",
    "}\n",
    "\n",
    "# Fetch the top 50 hits for each year\n",
    "top_hits = {}\n",
    "for year, playlist_id in playlist_ids.items():\n",
    "    top_hits[year] = get_playlist_tracks(playlist_id)\n",
    "    time.sleep(1)  # To avoid hitting rate limits"
