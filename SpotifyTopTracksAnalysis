    {
     "data": {
      "text/plain": [
       "dict_keys([2013, 2014, 2015, 2016, 2017, 2018, 2019, 2020, 2021, 2022, 2023])"
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Append the 'global_top_50' DataFrame to the 'top_hits' dictionary\n",
    "# We will treat these songs as the top hits of 2023\n",
    "top_hits[2023] = global_top_50\n",
    "# Check the keys of the dictionary to see if it contains the recently appended dataframe for 2023\n",
    "top_hits.keys()"
   ]
