    "    'acousticness': 'Acousticness',\n",
    "    'valence': 'Valence',\n",
    "    'tempo': 'Tempo'\n",
    "}\n",
    "\n",
    "# Iterate over the dictionary and rename the columns\n",
    "for year in top_hits.keys():\n",
    "    top_hits[year] = top_hits[year].rename(columns=new_column_names)\n",
    "    \n",
