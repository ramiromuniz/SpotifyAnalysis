    "# Lets define a dictionary for the new column names\n",
    "new_column_names = {\n",
    "    'id': 'ID',\n",
    "    'name' : 'Name',\n",
    "    'artist': 'Artist',\n",
    "    'duration_ms': 'Duration',\n",
    "    'popularity': 'Popularity',\n",
    "    'danceability': 'Danceability',\n",
    "    'energy': 'Energy',\n",
